{
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "family": "LearnJenkinsApp-MyTaskDefinition-Prod",
  "containerDefinitions": [
    {
      "name": "learnjenkinsapp",
      "image": "635928276728.dkr.ecr.eu-north-1.amazonaws.com/learnjenkinsapp:1.0.133",
      "portMappings": [
        {
          "name": "ngnix-80-tip",
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true
    }
  ],
  "volumes": [],
  "networkMode": "awsvpc",
  "memory": "512",
  "cpu": "256",
  "executionRoleArn": "arn:aws:iam::635928276728:role/ecsTaskExecutionRole"
}